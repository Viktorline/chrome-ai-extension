{
  "trailingComma": "none",
  "semi": false,
  "jsxSingleQuote": true,
  "singleQuote": true,
  "arrowParens": "avoid",
  "tabWidth": 2,
  "importOrder": [
    "globals.css$",
    "<THIRD_PARTY_MODULES>",
    "(^@/components)|(^@/layouts)|(^@/constants)|(^@/content)|(^@/hooks)|(^@/http)|(^@/services)|(^@/store)|(^@/utils)|(^@/views)",
    "(^@/models)|(^@/props)|(.props$)",
    ".css$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "plugins": ["@trivago/prettier-plugin-sort-imports"]
}
